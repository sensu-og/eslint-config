{
  "name": "eslint-config-sensu",
  "version": "1.0.1",
  "description": "Sensu's ESLint config",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sensu/eslint-config-sensu/packages/eslint-config-sensu"
  },
  "bugs": {
    "url": "https://github.com/sensu/eslint-config-sensu/issues"
